<h1>Listing tickets</h1>

<table border="1">
  <tr>
    <th>ID</th>
    <th>Caller-Number </th>
    <th>Kookoo-Number </th>
    <th>Called-number </th>
    <th>Caller-location </th>
  </tr>

<% @tickets.each do |ticket| %>
  <tr>
    <td><%= ticket.id %></td>
    <td><%= ticket.caller_number %> </td>
    <td><%= ticket.kookoo_number %> </td>
    <td><%= ticket.called_number %> </td>
    <td><%- ticket.location %> </td>
    <td><%= link_to 'Delete', ticket, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

